{
	"name": "polkadot-runtime-prom-exporter",
	"version": "0.12.0",
	"description": "Prometheus exporter for polkadot runtime metrics",
	"scripts": {
		"dev": "nodemon",
		"run": "yarn run build && node build/index.js",
		"build": "./node_modules/.bin/rimraf ./build && ./node_modules/.bin/tsc",
		"binary": "yarn run build && rimraf ./binaries &&./node_modules/.bin/nexe build/index.js --build -o binaries/polkadot-runtime-prom-exporter",
		"publish": "npm publish",
		"update": "./node_modules/.bin/ncu -u && yarn"
	},
	"author": "@kianenigma",
	"license": "ISC",
	"devDependencies": {
		"@babel/cli": "^7.17.10",
		"@babel/core": "^7.17.12",
		"@babel/preset-typescript": "^7.17.12",
		"@types/node": "^17.0.34",
		"nexe": "4.0.0-rc.1",
		"nodemon": "^2.0.16",
		"npm-check-updates": "13.0.1",
		"rimraf": "^3.0.2",
		"ts-loader": "^9.3.0",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.4",
		"webpack": "^5.72.1",
		"webpack-cli": "^4.9.2"
	},
	"dependencies": {
		"@polkadot/api": "8.4.2",
		"bn.js": "^5.2.0",
		"dotenv": "^16.0.1",
		"express": "^4.18.1",
		"pg": "^8.7.3",
		"pg-hstore": "^2.3.4",
		"prom-client": "^14.0.1",
		"sequelize": "^6.21.3",
		"sequelize-cli": "^6.4.1",
		"winston": "^3.7.2"
	}
}
